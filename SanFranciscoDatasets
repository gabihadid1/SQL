<div class="highlight hl-ipython3"><pre><span></span><span class="c1"># Insert query below between the two triple-parenthesis</span>
<span class="n">QUERY</span> <span class="o">=</span> <span class="p">(</span><span class="s2">"""SELECT * </span>
<span class="s2">            FROM </span><span class="si">{}</span><span class="s2"> </span>
<span class="s2">            LIMIT 1000 """</span><span class="o">.</span><span class="n">format</span><span class="p">(</span><span class="n">san_fan_films_locs</span><span class="p">))</span>
